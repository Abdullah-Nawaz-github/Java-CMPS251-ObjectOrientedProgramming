						<TableColumn fx:id="idCol" maxWidth="1.7976931348623157E308" prefWidth="101.99999475479126" text="Session id" />

						<TableColumn fx:id="nameCol" maxWidth="1.7976931348623157E308" prefWidth="101.99999475479126" text="Session Name" />
